<% provide(:title, 'Events') %>
<h1>Events</h1>


<%= button_to 'Create New Event', new_event_path, class: "btn btn-block btn-large btn-success", :method => :get %>
<%= button_to 'See My Events', my_events_path, class: "btn btn-block btn-large btn-danger", :method => :get  %>


<ol class="events">
   <%= render partial: 'shared/events/event', collection: @events %>
 </ol>
<%= will_paginate @events %>