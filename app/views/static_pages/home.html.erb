<% provide(:title, 'Home') %>

<% if signed_in? %>
  <% if !current_user.events.empty? %> 
      <ol class="events">
        <%= render partial: 'shared/events/event', collection: @events %>
      </ol>
   <%else%>
     <%= render 'noevents'%>
   <%end%>
<%else%>
 <%= render 'unsigned'%>
<%end%>     